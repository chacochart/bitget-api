    "@types/node": "^20.12.11",
    "cross-env": "^7.0.3",
    "eslint": "^9.2.0",
    "jest": "^29.7.0",
    "shx": "^0.3.4",
    "source-map-loader": "^5.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
